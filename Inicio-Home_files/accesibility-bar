(function(){function u(){window.scrollTo({top:0,behavior:"smooth"})}function n(){var n=window.localStorage.getItem("govco:accesibility:fontsize"),t,i;return n?parseFloat(n):(t=document.querySelector("html"),i=window.getComputedStyle(t).getPropertyValue("font-size"),parseFloat(i))}function t(n){var t=n.toString()+"px";document.querySelector("html").style.fontSize=t;window.localStorage.setItem("govco:accesibility:fontsize",t)}function f(){var i=n();t(i)}function e(){var i=n();i>8&&t(i-1)}function o(){var i=n();i<48&&t(i+1)}function i(){var n=window.localStorage.getItem("govco:accesibility:contrastmode");return n?n==="true"?!0:!1:!1}function r(n=false){window.localStorage.setItem("govco:accesibility:contrastmode",n?"true":"false");n?document.querySelector("html").classList.contains("contrast-black")||document.querySelector("html").classList.add("contrast-black"):document.querySelector("html").classList.contains("contrast-black")&&document.querySelector("html").classList.remove("contrast-black")}function s(){var n=!i();r(n)}function h(){var n=i();r(n)}function c(){var n=document.querySelector(".accessibility-bar");if(n){var t=n.querySelector(".min-fontsize"),i=n.querySelector(".max-fontsize"),r=n.querySelector(".contrast-ref"),u=document.querySelector(".scroll-to-top-btn");t&&t.addEventListener("click",n=>{n.preventDefault(),document.dispatchEvent(new Event("decreaseFontSize"))});i&&i.addEventListener("click",n=>{n.preventDefault(),document.dispatchEvent(new Event("increaseFontSize"))});r&&r.addEventListener("click",n=>{n.preventDefault(),document.dispatchEvent(new Event("toggleContrast"))});u&&u.addEventListener("click",n=>{n.preventDefault(),document.dispatchEvent(new Event("scrollToTop"))});document.querySelector("body").appendChild(n);document.dispatchEvent(new Event("resetFontSize"));document.dispatchEvent(new Event("initialContrast"))}}document.addEventListener("DOMContentLoaded",()=>{document.addEventListener("increaseFontSize",()=>o()),document.addEventListener("decreaseFontSize",()=>e()),document.addEventListener("resetFontSize",()=>f()),document.addEventListener("scrollToTop",()=>u()),document.addEventListener("toggleContrast",()=>s()),document.addEventListener("initialContrast",()=>h()),c()})})()